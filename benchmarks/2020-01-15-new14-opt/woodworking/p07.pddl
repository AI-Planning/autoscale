    (wood b0 teak)
    (surface-condition b0 rough)
    (available b0)
    (boardsize b1 s0)
    (wood b1 teak)
    (surface-condition b1 rough)
    (available b1)
    (boardsize b2 s4)
    (wood b2 cherry)
    (surface-condition b2 smooth)
    (available b2)
