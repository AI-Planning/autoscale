    (colour p4 red)
    (surface-condition p4 verysmooth)
    (available p5)
    (colour p5 red)
    (surface-condition p5 smooth)
    (available p6)
    (treatment p6 varnished)
    (wood p6 teak)
    (colour p6 natural)
    (surface-condition p6 smooth)
    )
  )
  (:metric minimize (total-cost))
)
