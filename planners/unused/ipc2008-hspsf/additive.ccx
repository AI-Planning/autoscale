void AH::write_eval(const index_set& s, std::ostream& st, char* p, bool e)
{
  hsps::rational v_max = (Hmax ? Hmax->eval(s) : 0);
  hsps::rational v_sum = 0;
  if (p) st << p << " (";
  for (index_type k = 0; k < H_vec.length(); k++) {
    assert(H_vec[k]);
    v_sum += H_vec[k]->eval(s);
    if (k > 0) st << " ";
    st << H_vec[k]->eval(s);
  }
  st << ") " << v_sum << " " << v_max;
  if (e) st << std::endl;
}
