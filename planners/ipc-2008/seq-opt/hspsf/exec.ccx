void Schedule::writeGantt(::std::ostream& s) const
{
  double pic_width = makespan().decimal();
  double pic_height = (n_tracks + 2);
  s << "\\newcommand{\\ganttunitwidth}{" << GANTT_UNIT_WIDTH << "}"
    << ::std::endl;
  s << "\\newcommand{\\ganttunitheight}{" << GANTT_UNIT_HEIGHT << "}"
    << ::std::endl;
