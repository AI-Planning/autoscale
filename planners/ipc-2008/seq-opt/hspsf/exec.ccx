}
void ExecError::write(::std::ostream& s) const
{
  s << '[' << std::resetiosflags(std::ios::scientific) << ((time_of_error()).decimal());
  if (step_of_error() != no_such_index) {
    s << " (step #" << step_of_error() << ")";
  }
  else {
    s << " (no step)";
  }
  s << ']' << error_type_string(type_of_error());
}
