    s << "\\advance\\x by \\gantttextxoff" << ::std::endl;
    s << "\\y=" << y0 << "\\ganttunitheight" << ::std::endl;
    s << "\\advance\\y by \\gantttextyoff" << ::std::endl;
    if (GANTT_ACTION_NAMES_ON_CHART) {
