                    lazy_wastar(hff2,hlm2,preferred=(hff2,hlm2),w=2),
                    lazy_wastar(hff2,hlm2,preferred=(hff2,hlm2),w=1),
                    repeat_last=true,continue_on_fail=true)" \
                "$@" < $TEMPFILE
        else
            echo "Something is seriously messed up!"
            exit 2
        fi
    elif [[ "$CONFIG" == "seq-sat-jasper" ]]; then
        if [[ "$UNIT_COST" == "unit" ]]; then
            "$PLANNER" \
                --heuristic "hlm,hff=lm_ff_syn(lm_rhw(
                    reasonable_orders=true,lm_cost_type=2,cost_type=2))" \
                --search "iterated_aras_rw(
                    lazy_two_level_macro_type_greedy(hff,hlm,preferred=(hff,hlm)),
                    lazy_wastar(hff,hlm,preferred=(hff,hlm),w=5),
                    lazy_wastar(hff,hlm,preferred=(hff,hlm),w=3),
                    lazy_wastar(hff,hlm,preferred=(hff,hlm),w=2),
                    lazy_wastar(hff,hlm,preferred=(hff,hlm),w=1),
                    repeat_last=true,continue_on_fail=true)" \
                "$@" < $TEMPFILE
        elif [[ "$UNIT_COST" == "nonunit" ]]; then
            "$PLANNER" \
                --heuristic "hlm1,hff1=lm_ff_syn(lm_rhw(
                    reasonable_orders=true,lm_cost_type=1,cost_type=1))" \
                --heuristic "hlm2,hff2=lm_ff_syn(lm_rhw(
                    reasonable_orders=true,lm_cost_type=2,cost_type=2))" \
                --search "iterated_aras_rw(
                    lazy_two_level_macro_type_greedy(hff1,hlm1,preferred=(hff1,hlm1)),
                    lazy_greedy(hff2,hlm2,preferred=(hff2,hlm2),
                                reopen_closed=false),
                    lazy_wastar(hff2,hlm2,preferred=(hff2,hlm2),w=5),
                    lazy_wastar(hff2,hlm2,preferred=(hff2,hlm2),w=3),
                    lazy_wastar(hff2,hlm2,preferred=(hff2,hlm2),w=2),
                    lazy_wastar(hff2,hlm2,preferred=(hff2,hlm2),w=1),
                    repeat_last=true,continue_on_fail=true)" \
                "$@" < $TEMPFILE
        else
            echo "Something is seriously messed up!"
            exit 2
        fi
    elif [[ "$CONFIG" == "seq-agl-jasper" ]]; then
        if [[ "$UNIT_COST" == "unit" ]]; then
            "$PLANNER" \
                --heuristic "hlm,hff=lm_ff_syn(lm_rhw(
                    reasonable_orders=true,lm_cost_type=2,cost_type=2))" \
                --search "lazy_two_level_macro_type_greedy(hff,hlm,preferred=(hff,hlm))" \
                "$@" < $TEMPFILE
        elif [[ "$UNIT_COST" == "nonunit" ]]; then
            "$PLANNER" \
                --heuristic "hlm1,hff1=lm_ff_syn(lm_rhw(
                    reasonable_orders=true,lm_cost_type=1,cost_type=1))" \
                --search "lazy_two_level_macro_type_greedy(hff1,hlm1,preferred=(hff1,hlm1),cost_type=1,reopen_closed=false)" \
                "$@" < $TEMPFILE
        else
            echo "Something is seriously messed up!"
            exit 2
        fi
    else
        echo "unknown IPC planner name: $CONFIG"
        exit 2
    fi
else
    "$PLANNER" "$@" < $TEMPFILE
fi
rm -f $TEMPFILE


